<%- include('../partials/header') %>

<h1 class="sectoin-heading"></h1>
<div class="event-list">
  <% events.forEach(function(e) { %>
  <article class="event">
    <img
    class="event-image"
      loading="lazy"
      src="https://images.unsplash.com/photo-1544367567-0f2fcb009e0b?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxleHBsb3JlLWZlZWR8Mnx8fGVufDB8fHx8&w=1000&q=80"
      alt=" "
    />
    <h1 class="event-name primary-headline"><%= e.title %></h1>
    <p class="event-date"><%= e.eventDate.toLocaleDateString() %></p>
    <p class="event-category meta">
      <%= e.eventCategory%> </p>
    <a class="a-details" href="/events/<%= e._id %>">Details</a>
    
  </article>
  <% }); %>
</div>

<%- include('../partials/footer') %>
